+  hex2bytes(hex_str, sib8->serialNumber.buf, 2);
+  sib8->serialNumber.bits_unused = 0;
+
+  sib8->warningMessageSegmentType = NR_SIB8__warningMessageSegmentType_lastSegment;
+  sib8->warningMessageSegmentNumber = 0;
+
+
+  sib8->dataCodingScheme = CALLOC(1, sizeof(OCTET_STRING_t));
+  sib8->dataCodingScheme->size = 1;
+  sib8->dataCodingScheme->buf = CALLOC(1, 1);
+  get_config_value(config_file, "dataCodingScheme", hex_str, strlen("dataCodingScheme"));
+  hex2bytes(hex_str, sib8->dataCodingScheme->buf, 1);
+
+  char alert_buffer[100];
+  get_config_value(config_file, "text", alert_buffer, strlen("text"));
+
+
+  uint8_t segment[84] = {0};
+  // Number of Pages
+  segment[0] = 0x01;
+
+
+  if( sib8->dataCodingScheme->buf[0] == 0x01) {
+    // Encode text as GSM 7-bit
+    gsm_7bit_encode_n(segment+1, 82, alert_buffer, segment+83);
+  }
+  else if(sib8->dataCodingScheme->buf[0] == 0x11){
+    // Encode text as UCS-2
+    // Language indicator (GSM 7-bit)
+    char lan[5];
+    get_config_value(config_file, "lan", lan, strlen("lan"));
+    hex2bytes(lan, segment+1, 2);
+
+    segment[83] = utf8_to_ucs2(alert_buffer, segment + 3) + 3;
+  } 
+
+  sib8->warningMessageSegment.size = 84;
+  // sib8->warningMessageSegment.buf = segment;
+  sib8->warningMessageSegment.buf = CALLOC(84, sizeof(uint8_t));
+  memcpy(sib8->warningMessageSegment.buf, segment, 84);
+
