   AssertFatal (size > 0, "ASN1 message encoding failed (encoded %lu bytes)!\n", size);
-  ASN_STRUCT_FREE(asn_DEF_NR_SIB2, sib2);
+  ASN_STRUCT_FREE(asn_DEF_NR_SIB8, sib8);
   return size;
 }
 
